    <div class="row">
        <div class="mx-auto my-3">
         <%= image_tag 'eco.png', size: '300x100' %>
         </div>
    </div>
    <h2> 新着商品</h2>
    <div class="row">
        <% @items.each do |item| %>
       <div class="mx-auto ">
       <%= attachment_image_tag item, :image, format: 'jpeg',size: "200x100" %><br>
       <%= item.name %><br>
       ￥<%= (item.price*1.1).floor %>
       </div>
       <% end %>
    </div>
    <%= link_to "全ての商品を見る",items_path %>