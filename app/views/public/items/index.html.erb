    <big class="silver">商品一覧</big>(全<%= @items.count %>件)
    <div class="row">
       <% @items.each do |item| %>
       <div class="col-lg-3 my-3">
       <%= attachment_image_tag item, :image, format: 'jpeg',size: "200x100" %><br>
       <%= link_to item.name,item_path(item) %><br>
       ￥<%= (item.price*1.1).floor %>
       </div>
       <% end %>
    </div>