    <big class="silver"><%= @customer.all_name%>さんの会員情報編集</big>
    <%= form_with model: @customer,url: admin_customer_path(@customer),local: true do |f| %>
　  <div class="row">
　    <div class="col-3">
　      <spna>名前</span>
　    </div>
　    <div class="col-3">
          <span>姓</span>
          <%= f.text_field :last_name, autofocus: true,class: "form-control"  %>
　    </div>
　    <div class="col-3">
        <span>名</span>
        <%= f.text_field :first_name, autofocus: true,class: "form-control" %>
　    </div>
　  </div>
　  
　  <div class="row">
　    <div class="col-3">
　      <spna>フリガナ</span>
　    </div>
　    <div class="col-3">
          <%= f.label "セイ" %>
     　　　　<%= f.text_field :last_name_kana, autofocus: true,class: "form-control"  %>
　    </div>
　    <div class="col-3">
          <%= f.label "メイ" %>
          <%= f.text_field :first_name_kana,autofocus: true,class: "form-control" %>
　    </div>
　   </div>
　    <div class="row">
　    <div class="col-3">
　      <spna>メールアドレス</span>
　    </div>
　    <div class="col-3">
    <%= f.email_field :email, autofocus: true, autocomplete: "email",class: "form-control"  %>
　    </div>
　   </div>
　   <div class="row">
　    <div class="col-3">
　      <spna>郵便番号(ハイフンなし)</span>
　    </div>
　    <div class="col-3">
    <%= f.text_field :zip_code, autofocus: true,class: "form-control"  %>
　    </div>
　   </div>
   　<div class="row">
　    <div class="col-3">
　      <spna>住所</span>
　    </div>
　    <div class="col-6">
     　<%= f.text_field :address, autofocus: true ,class: "form-control" %>
　    </div>
　   </div>
　    <div class="row">
　    <div class="col-3">
　      <spna>電話番号(ハイフンなし)</span>
　    </div>
　    <div class="col-3">
      <%= f.text_field :telephone_number, autofocus: true ,class: "form-control" %>
　    </div>
　   </div>　
　    <div class="row">
　    <div class="col-3">
　      <spna>会員ステータス</span>
　    </div>
　    <div class="col-3">
      <%= f.radio_button :is_deleted,false %>有効
　    </div>
　     <div class="col-3">
      <%= f.radio_button :is_deleted,true%>退会
　    </div>
　   </div>
　   <div class="row">
　     <div class="col-3 offset-3">
       <%= f.submit "保存",class: "btn btn-success btn-block" %>
　     </div>
　   </div>
    <% end %>